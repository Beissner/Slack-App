{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "indent": ["off", "spaces"],
    "linebreak-style": ["error", "unix"],
    "quotes": ["off", "double"],
    "semi": ["error", "always"],
    "no-console": ["off"],
    "react/prop-types": "off"
  }
}
